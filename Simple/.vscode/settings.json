// Place your settings in this file to overwrite default and user settings.
{
	"editor.formatOnType": true,
	"eslint.enable": true,	
	"files.associations": {
		".babelrc": "json"
	},
	"files.exclude": {
		".vscode/chrome/": true
	},
	"files.watcherExclude": {
		".vscode/chrome/": true
	},
	"html.format.extraLiners": "!",
	"html.format.indentInnerHtml": true,
	"search.exclude": {
		"dist/": true
	},
	"json.schemas": [
        {
            "fileMatch": [
                "/translations/*.json"
            ],
            "url": "./translation.schema.json"
        }
    ]
}