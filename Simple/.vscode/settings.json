{
	"editor.formatOnType": true,
	"eslint.enable": true,
	"files.associations": {
		".babelrc": "json"
	},
	"files.exclude": {
		".vscode/chrome/": true
	},
	"files.watcherExclude": {
		".vscode/chrome/": true
	},
	"html.format.extraLiners": "!",
	"html.format.indentInnerHtml": true,
	"search.exclude": {
		"dist/": true
	},
	"json.schemas": [
		{
			"fileMatch": [
				"/translations/*.json"
			],
			"url": "./translation.schema.json"
		}
	],
	"i18nTag.src": "./src",
	"i18nTag.schema": "./translation.schema.json",
	"i18nTag.filter": "\\.jsx?"
}